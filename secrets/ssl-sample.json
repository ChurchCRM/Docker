[{
        "ssl": "letsencrypt",
        "host": "www.example.com",
        "letsencrypt": {
            "mode": "dns",
            "domain_parameter": "-d www.example.com",
            "extra_parameter": "--dnssleep 60 --keylength ec-256 --log /var/log/nginx/acme.log"
        }
    },
    {
        "ssl": "letsencrypt",
        "host": "www.example1.com",
        "letsencrypt": {
            "mode": "webroot",
            "domain_parameter": "-d www.example1.com",
            "extra_parameter": "--keylength ec-256 --log /var/log/nginx/acme.log"
        }
    },
    {
        "ssl": "build",
        "host": "myapp.localtest.me",
        "build": {
            "country": "US",
            "state": "Tennessee",
            "locality": "Nashville",
            "organization": "ChurchCRM",
            "organizationalunit": "IT",
            "email": "admin@example.com"
        }
    },
    {
        "ssl": "none",
        "host": "www.example2.com"
    }
]