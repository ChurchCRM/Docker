[req]
default_bits = 2048
prompt = no
default_md = sha256
distinguished_name = dn

[dn]
C=US                            # Country Name (2 letter code)
ST=Tennessee                    # State or Province Name (full name)
L=Nashville                     # Locality Name (eg, city)
O=End Point                     # Organization Name (eg, company)
OU=Testing Domain               # Organizational Unit Name (eg, section)
emailAddress=email@domain.com   # Your E-Mail Address
CN = localhost                  # Common Name (e.g. server FQDN or YOUR name)
